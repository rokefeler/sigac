  <!-- Modules -->
  
  <import file="${basedir}/module_sigac.xml"/>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.sigac, clean.artifact.sigac:war_exploded" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.sigac" description="build all modules"/>
  
  <target name="init.artifacts">
    <property name="artifacts.temp.dir" value="${basedir}/__artifacts_temp"/>
    <property name="artifact.output.sigac:war" value="${basedir}/target"/>
    <property name="artifact.output.sigac:war_exploded" value="${basedir}/target/sigac-1.0"/>
    <mkdir dir="${artifacts.temp.dir}"/>
    <property name="temp.jar.path.sigac-1.0.war" value="${artifacts.temp.dir}/sigac-1.0.war"/>
  </target>
  
  <target name="clean.artifact.sigac:war_exploded" description="clean sigac:war exploded artifact output">
    <delete dir="${artifact.output.sigac:war_exploded}"/>
  </target>
  
  <target name="artifact.sigac:war" depends="init.artifacts, artifact.sigac:war_exploded" description="Build &#39;sigac:war&#39; artifact">
    <property name="artifact.temp.output.sigac:war" value="${artifacts.temp.dir}/sigac_war"/>
    <mkdir dir="${artifact.temp.output.sigac:war}"/>
    <zip destfile="${temp.jar.path.sigac-1.0.war}">
      <zipfileset dir="${artifact.output.sigac:war_exploded}"/>
    </zip>
    <copy file="${temp.jar.path.sigac-1.0.war}" tofile="${artifact.temp.output.sigac:war}/sigac-1.0.war"/>
  </target>
  
  <target name="artifact.sigac:war_exploded" depends="init.artifacts, compile.module.sigac" description="Build &#39;sigac:war exploded&#39; artifact">
    <mkdir dir="${artifact.output.sigac:war_exploded}"/>
    <mkdir dir="${artifact.output.sigac:war_exploded}/WEB-INF"/>
    <mkdir dir="${artifact.output.sigac:war_exploded}/WEB-INF/classes"/>
    <copy todir="${artifact.output.sigac:war_exploded}/WEB-INF/classes">
      <fileset dir="${sigac.output.dir}"/>
    </copy>
    <mkdir dir="${artifact.output.sigac:war_exploded}/WEB-INF/classes/META-INF"/>
    <copy file="${basedir}/src/main/resources/META-INF/persistence.xml" tofile="${artifact.output.sigac:war_exploded}/WEB-INF/classes/META-INF/persistence.xml"/>
    <mkdir dir="${artifact.output.sigac:war_exploded}/WEB-INF/lib"/>
    <copy file="${path.variable.maven_repository}/org/hibernate/hibernate-validator/5.0.1.Final/hibernate-validator-5.0.1.Final.jar" tofile="${artifact.output.sigac:war_exploded}/WEB-INF/lib/hibernate-validator-5.0.1.Final.jar"/>
    <copy file="${path.variable.maven_repository}/javax/validation/validation-api/1.1.0.Final/validation-api-1.1.0.Final.jar" tofile="${artifact.output.sigac:war_exploded}/WEB-INF/lib/validation-api-1.1.0.Final.jar"/>
    <copy file="${path.variable.maven_repository}/org/jboss/logging/jboss-logging/3.1.1.GA/jboss-logging-3.1.1.GA.jar" tofile="${artifact.output.sigac:war_exploded}/WEB-INF/lib/jboss-logging-3.1.1.GA.jar"/>
